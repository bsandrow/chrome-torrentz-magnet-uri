{
  "name": "Torrentz Magnet Link",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Add a magnet link to the torrent pages on Torrentz.eu.",
  "icons": { "128": "images/icon-128.png" },
  "background": { "page": "background.html" },
  "page_action": {
    "default_icon": "images/icon-128.png",
    "default_title": "Magnet URI"
  },
  "permissions": [
    "tabs",
    "http://*.torrentz.eu/*",
    "https://*.torrentz.eu/*"
  ]
}
